{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/docs/:path*",
      "destination": "/docs/index.html"
    },
    {
      "source": "/demos/:path*",
      "destination": "/demos/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/old-docs",
      "destination": "/docs",
      "permanent": true
    },
    {
      "source": "/react-ui-library",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/best-react-components",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|webp|svg|gltf|obj|woff2?|ttf|otf))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
